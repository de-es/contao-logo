{
    "name": "de-es/contao-logo",
    "description": "Contao extension providing a content element and frontend module for directly embedding a single logo image",
    "type": "contao-bundle",
    "license": "LGPL-3.0-or-later",
    "homepage": "https://github.com/de-es/contao-logo",
    "authors": [
        {
            "name": "Dennis Schmidt",
            "homepage": "https://de-es.de",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/de-es/contao-logo/issues",
        "source": "https://github.com/de-es/contao-logo"
    },
    "require": {
        "php": ">=8.1",
        "contao/core-bundle": "^5.3"
    },
    "require-dev": {
        "contao/easy-coding-standard": "^6.0",
        "contao/rector": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "DeEs\\ContaoLogo\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "contao-components/installer": true,
            "php-http/discovery": false,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "contao-manager-plugin": "DeEs\\ContaoLogo\\ContaoManager\\Plugin"
    },
    "scripts": {
        "all": [
            "@ecs",
            "@rector"
        ],
        "ecs": "@php vendor/bin/ecs check --fix",
        "rector": "@php vendor/bin/rector"
    }
}
